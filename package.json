    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "msw": "^0.35.0",
    "node-notifier": "^10.0.1",
    "patch-package": "^6.4.7",
