{
  "name": "mui-storybook-design-system",
  "version": "0.0.0",
  "license": "MIT",
  "main": "any/index.js",
  "files": [
    "anyscript"
  ],
  "scripts": {
    "clean": "rimraf dist node_modules yarn.lock",
    "format": "prettier --write \"**/*.+(json|yml|css|md|mdx)\"",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx --fix",
    "release": "dotenv yarn build & yarn typescript:generate && auto shipit",
    "spj": "sort-package-json",
    "storybook": "start-storybook -p 6006",
    "build": "build-storybook",
    "test": "jest",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "typescript:generate": "tsc --declaration --emitDeclarationOnly --outDir dist --declarationMap",
    "validate": "./scripts/validate"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "dependencies": {},
