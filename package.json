{
  "name": "mui-storybook-design-system",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "build-storybook",
    "clean": "rimraf dist node_modules yarn.lock",
    "format": "prettier --write \"**/*.+(json|yml|css|md|mdx)\"",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx --fix",
    "sb:upgrade": "sb upgrade --prerelease",
    "test": "jest",
    "gen": "plop",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "typescript:generate": "tsc --declaration --emitDeclarationOnly --outDir dist --declarationMap",
    "sort-package-json": "sort-package-json",
    "validate": "./scripts/validate"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "dependencies": {},
  "devDependencies": {
    "@mui/icons-material": "^5.4.1",
